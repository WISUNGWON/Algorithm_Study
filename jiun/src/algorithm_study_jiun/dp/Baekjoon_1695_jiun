import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

// https://www.acmicpc.net/problem/1695
public class algorithm_study_jiun {

	private static int len;
	private static int[] arr;
	private static int[][] dp;

	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int n = Integer.parseInt(br.readLine());
		arr = new int[n];
		dp = new int[n][n];
		StringTokenizer st = new StringTokenizer(br.readLine());
		for (int i = 0; i < n; i++) {
			arr[i] = Integer.parseInt(st.nextToken());
		}
		System.out.println(isPallin(0, arr.length - 1));
	}

	private static int isPallin(int start, int end) {
		if (start >= end)
			return 0;
		if(dp[start][end]>0) //값이있으면 
			return dp[start][end];
		if (arr[start] == arr[end]) {
			return isPallin(start+1, end -1);
		} 
			
		return dp[start][end]=Math.min(isPallin(start+1, end)+1, isPallin(start, end-1)+1);
	}
}
